{
  "name": "@m1heng-clawd/feishu",
  "version": "0.1.0",
  "type": "module",
  "description": "Clawdbot Feishu/Lark channel plugin",
  "scripts": {
    "feishu:ws-online": "tsx scripts/feishu-ws-online.ts"
  },
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "clawdbot.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m1heng/clawd-feishu.git"
  },
  "keywords": [
    "clawdbot",
    "feishu",
    "lark",
    "飞书",
    "chatbot",
    "ai",
    "claude"
  ],
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "feishu",
      "label": "Feishu",
      "selectionLabel": "Feishu/Lark (飞书)",
      "docsPath": "/channels/feishu",
      "docsLabel": "feishu",
      "blurb": "飞书/Lark enterprise messaging.",
      "aliases": [
        "lark"
      ],
      "order": 70
    },
    "install": {
      "npmSpec": "@m1heng-clawd/feishu",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.30.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "clawdbot": "2026.1.24-2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "clawdbot": ">=2026.1.24"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
